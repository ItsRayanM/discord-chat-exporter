{
  "name": "@rayanbazbouz/discord-chat-exporter",
  "version": "1.0.0",
  "description": "High-fidelity Discord chat exporter for bots with multi-format outputs",
  "main": "./dist/index.js",
  "scripts": {
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "tsc -p tsconfig.json && npx tsc-alias -p tsconfig.json",
    "prepublishOnly": "npm run typecheck && npm run test && npm run build",
    "dev": "tsx src/cli.ts",
    "clean": "node scripts/clean.mjs"
  },
  "keywords": [
    "discord",
    "transcript",
    "exporter",
    "discordjs",
    "tickets",
    "chat"
  ],
  "author": "",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "@fast-csv/format": "^5.0.5",
    "archiver": "^7.0.1",
    "commander": "^14.0.2",
    "discord.js": "^14.25.1",
    "fast-xml-parser": "^5.3.6",
    "markdown-it": "^14.1.1",
    "p-limit": "^7.3.0",
    "zod": "^4.3.6"
  },
  "optionalDependencies": {
    "@aws-sdk/client-s3": "^3.940.0",
    "@azure/storage-blob": "^12.29.1",
    "@google-cloud/storage": "^7.17.3",
    "@slack/webhook": "^7.0.5",
    "archiver-zip-encrypted": "^2.0.0",
    "better-sqlite3": "^12.6.2",
    "docx": "^9.5.3",
    "flexsearch": "^0.8.212",
    "json-canonicalize": "^2.0.0",
    "libphonenumber-js": "^1.12.24",
    "mongodb": "^7.0.0",
    "mongoose": "^8.19.1",
    "mysql2": "^3.15.3",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "playwright": "^1.58.2",
    "shiki": "^3.22.0",
    "validator": "^13.15.20"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/archiver": "^6.0.3",
    "@types/better-sqlite3": "^7.6.13",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.18.6",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "eslint": "^10.0.0",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "bin": {
    "dcexport": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
